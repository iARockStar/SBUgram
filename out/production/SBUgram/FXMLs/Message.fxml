<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="root" prefHeight="81.0" prefWidth="454.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="usernameLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="325.0" layoutY="3.0" text="username">
         <font>
            <Font name="Georgia Bold Italic" size="18.0" />
         </font>
      </Label>
      <VBox layoutX="66.0" layoutY="27.0" prefWidth="362.0">
         <children>
            <Label fx:id="textLabel" alignment="TOP_LEFT" prefWidth="362.0" style="-fx-background-color: #32dc32;" text="message">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
               <effect>
                  <DropShadow />
               </effect>
            </Label>
         </children></VBox>
      <Label fx:id="dateLabel" layoutX="14.0" layoutY="5.0" text="Date and Time" />
      <ImageView fitHeight="22.0" fitWidth="36.0" layoutX="397.0" layoutY="56.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/waste_512px.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <DropShadow />
         </effect>
      </ImageView>
      <ImageView fitHeight="18.0" fitWidth="22.0" layoutX="372.0" layoutY="58.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/edit_480px.png" />
         </image>
         <effect>
            <DropShadow />
         </effect>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <Button layoutX="396.0" layoutY="54.0" mnemonicParsing="false" onAction="#deleteMessage" opacity="0.0" prefHeight="18.0" prefWidth="6.0" text="Button">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button layoutX="369.0" layoutY="54.0" mnemonicParsing="false" onAction="#editMessage" opacity="0.0" prefHeight="18.0" prefWidth="5.0" text="Button">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>

</AnchorPane>

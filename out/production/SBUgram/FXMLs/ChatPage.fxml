         <Cursor fx:constant="HAND" />
      </cursor>
   </Button>
   <Button layoutX="400.0" layoutY="553.0" mnemonicParsing="false" onAction="#attach" opacity="0.0" prefHeight="48.0" prefWidth="52.0" text="Button">
      <cursor>
         <Cursor fx:constant="HAND" />
      </cursor>
   </Button>
    <JFXListView fx:id="chatListView" layoutY="158.0" prefHeight="345.0" prefWidth="452.0" />

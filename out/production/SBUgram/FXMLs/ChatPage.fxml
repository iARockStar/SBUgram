   <ImageView fitHeight="48.0" fitWidth="54.0" layoutX="402.0" layoutY="500.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@../images/paper_plane_208px.png" />
      </image>
   </ImageView>
   <ImageView fitHeight="50.0" fitWidth="48.0" layoutX="402.0" layoutY="553.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@../images/attach_500px.png" />
      </image>
   </ImageView>
